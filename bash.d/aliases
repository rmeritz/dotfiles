alias rm='/bin/rm -i'
alias gc='git commit -m'
alias gs='git status -sb'
alias ga='git add -p; gs'
alias proxy='title proxy; ssh -D 2001 -Nf mike-burns.com'
alias venv='workon `basename $PWD`'

alias sass_watch='title sass; cd ~/fundedbyme/fundedbyme/fundedbyme/static/css; sass --watch sass:. --style compressed'
alias foreman='title foreman; cd ~/fundedbyme/fundedbyme; venv; foreman start'
alias shell='title shell; cd ~/fundedbyme/fundedbyme; venv; ./manage.py shell'
alias dbshell='title psql; cd ~/fundedbyme/fundedbyme; venv; ./manage.py dbshell'
