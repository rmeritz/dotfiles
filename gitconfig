[init]
        templatedir = ~/.git_template
[user]
	name = Rebecca Meritz
	email = rebecca@meritz.com
[color]
	status = auto
        diff = auto
[core]
	excludesfile = /home/rebecca/.gitignore_global
[push]
        default = current
[gitsh]
        defaultCommand = status -s
	gitCommand = /usr/bin/hub
[alias]
        co = checkout
        an = add -N .
        a = add -p
        aa = add .
        c = commit -m
        cv = commit -v
        ca = commit --amend -v
        caa = commit --amend -C HEAD
        p = push
        s = status -s
        st = status
        pr = pull-request
        r = reset
        ctags = "!sh -c '[ -f .git/hooks/ctags ] || git init; .git/hooks/ctags' git-ctags"
[credential]
	helper = cache
[help]
  autocorrect = 1
