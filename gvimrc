set guioptions-=T
set guioptions-=m
set guioptions-=r
set guioptions-=L
set guioptions-=t
set mouse=
set lines=56
set columns=85
set gfn=Bitstream\ Vera\ Sans\ Mono\ 12
map <C-Z> <Nop>
highlight CursorLine guibg=#181818
highlight Normal guibg=Black guifg=White
set background=dark

map <Leader>+ :set gfn=Bitstream\ Vera\ Sans\ Mono\ 20<CR>
